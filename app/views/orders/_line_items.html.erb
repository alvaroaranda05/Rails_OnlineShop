<div class="field">
    <%= f.label :product_id %>
    <%= f.collection_select :product_id, Product.all, :id, :title %>

    <%= f.label :quantity %>
    <%= f.text_field :quantity %>

    <%= f.label :price %>
    <%= f.text_field :price %>

    <%= f.hidden_field :id %>

    <%= f.hidden_field :_destroy %>
    <img src="../assets/delete.png" width="20px" height="20px"/>
</div>